{
  "name": "gatsby-starter",
  "version": "1.0.0",
  "description": "Gatsby 1.0 starter",
  "author": "fabien0102 <fabien0102@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "test": "xo && tslint && jest",
    "test:watch": "jest --watch",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "graphql-types": "gql-gen --url http://localhost:8000/graphql --template typescript --out ./src/graphql-types.d.ts",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "@types/react": "15.0.21",
    "@types/react-dom": "0.14.23",
    "@types/react-helmet": "5.0.0",
    "babel-eslint": "^7.2.1",
    "change-case": "^3.0.1",
    "gatsby": "next",
    "gatsby-link": "next",
    "gatsby-plugin-glamor": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-typescript": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-json": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "gatsby-typegen-filesystem": "next",
    "gatsby-typegen-remark": "next",
    "gatsby-typegen-remark-autolink-headers": "next",
    "gatsby-typegen-remark-copy-linked-files": "next",
    "gatsby-typegen-remark-prismjs": "next",
    "gatsby-typegen-remark-responsive-image": "next",
    "gatsby-typegen-sharp": "next",
    "graphql-code-generator": "^0.5.1",
    "gray-matter": "^2.1.1",
    "react": "15.5.3",
    "react-helmet": "5.0.3",
    "semantic-ui-react": "^0.68.0",
    "slash": "1.0.0",
    "ts-loader": "^2.0.3",
    "typescript": "2.2.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-notes": "^1.0.1",
    "@kadira/storybook-addon-options": "^1.0.2",
    "@types/enzyme": "^2.7.8",
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.15",
    "@types/react-test-renderer": "^15.4.3",
    "enzyme": "^2.8.1",
    "eslint-config-xo-react": "0.11.1",
    "eslint-plugin-react": "6.10.3",
    "flat": "^2.0.1",
    "husky": "0.13.3",
    "jest": "19.0.2",
    "lint-staged": "3.4.0",
    "mkdirp": "^0.5.1",
    "plop": "^1.7.4",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "storybook-readme": "^2.0.2",
    "ts-jest": "19.0.8",
    "ts-lint": "4.5.1",
    "xo": "^0.18.1"
  },
  "keywords": [
    "gatsby",
    "starter"
  ],
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PREFIX_LINKS__": ""
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ],
    "*.md": [
      "node tools/update-post-date.js",
      "git add"
    ]
  },
  "xo": {
    "extends": "xo-react/space",
    "space": true
  }
}
